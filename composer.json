{
  "name": "cultuurnet/culturefeed-http-guzzle",
  "description": "Library with a Guzzle 3 replacement for the default, custom-made HTTP library of Culturefeed",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CultuurNet\\CulturefeedHttpGuzzle\\": "src"
    }
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\CulturefeedHttpGuzzle\\": "tests"
    }
  },
  "require": {
    "guzzle/guzzle": "~3.9.3",
    "guzzle/plugin-log": "~3.9.2",
    "cultuurnet/culturefeed-php": "~1.5.2",
    "psr/log": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "squizlabs/php_codesniffer": "~2.3",
    "phing/phing": "~2.11",
    "satooshi/php-coveralls": "~0.7"
  }
}
